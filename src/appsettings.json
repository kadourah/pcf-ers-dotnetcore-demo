{

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "Articulate": "Information"
    }
  },
  "Management": {

    "Endpoints": {
      "Enabled": true,
      "Path": "/actuators",
      "Cloudfoundry": {
        "ValidateCertificates": false
      },
      "Actuator": {
        "Exposure": {
          "Include": [
            "*"
          ]
        }
      }
    }
  },
  "Eureka": {
    "Client": {
      "Validate_certificates": false
    }
  },
  "tracing": {
    "alwaysSample": true,
    "useShortTraceIds ": true,
    "exporter": {
      "zipkin": {
        "endpoint": "http://wavefront-proxy.apps.kadourah.com:9411/api/v2/spans",
        "validateCertificates": false
      }

    }
  }
}
